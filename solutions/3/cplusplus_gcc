#include<bits/stdc++.h>
using namespace std;int main(){long long a=0,b=0;string s,t;for(;cin>>s;b+=stoll(t)){auto i=max_element(s.begin(),s.end()-1);a+=stoll(string{*i,*max_element(i+1,s.end())});int k=s.size()-12;t="";for(char c:s){for(;k&&t.size()&&t.back()<c;t.pop_back())k--;t+=c;}t.resize(12);}cout<<a<<' '<<b;}