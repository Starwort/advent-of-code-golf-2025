#include<bits/stdc++.h>
using namespace std;using ll=long long;int main(){ll a=0,b=0;string s;while(cin>>s){auto i=max_element(s.begin(),s.end()-1);a+=stoll(string{*i,*max_element(i+1,s.end())});int k=s.size()-12;string t;for(char c:s){while(k&&t.size()&&t.back()<c)k--,t.pop_back();t+=c;}t.resize(12);b+=stoll(t);}cout<<a<<' '<<b;}