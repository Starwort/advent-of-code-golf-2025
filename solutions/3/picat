import util.
main=>
    Lines = read_file_lines(),
    foreach(X in[2,12])
        Ans = 0,
        foreach(L in Lines)
            Cur = 0,
            N = L.len,
            Next = 1,
            foreach(I in 1..X)
                Max = max(L[Next..N-X+I]),
                Cur := Cur * 10 + [Max].to_int,
                Next := find_first_of(L[Next..N-X+I],Max)+Next
            end,
            Ans := Ans + Cur
        end,
        println(Ans)
    end.